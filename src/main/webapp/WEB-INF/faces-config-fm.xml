<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <converter>
        <converter-id>accPacCustomerConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.AccPacCustomerConverter</converter-class>
    </converter>
    <converter>
        <converter-id>activeSupplierConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.ActiveSupplierConverter</converter-class>
    </converter>
    <converter>
        <converter-id>accountingCodeConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.AccountingCodeConverter</converter-class>
    </converter>
    <converter>
        <converter-id>taxConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.TaxConverter</converter-class>
    </converter>
    <converter>
        <converter-id>discountConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.DiscountConverter</converter-class>
    </converter>
    <converter>
        <converter-id>classificationConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.ClassificationConverter</converter-class>
    </converter>
    <converter>
        <converter-id>jobCategoryConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.JobCategoryConverter</converter-class>
    </converter> 
    <converter>
        <converter-id>jobSubCategoryConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.JobSubCategoryConverter</converter-class>
    </converter> 
    <converter>
        <converter-id>sectorConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.SectorConverter</converter-class>
    </converter>
    <converter>
        <converter-id>serviceConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.ServiceConverter</converter-class>
    </converter>  
    
    <validator>
        <validator-id>accountingCodeValidator</validator-id>
        <validator-class>jm.com.dpbennett.fm.validator.AccountingCodeValidator</validator-class>
    </validator>  
    <validator>
        <validator-id>taxValidator</validator-id>
        <validator-class>jm.com.dpbennett.fm.validator.TaxValidator</validator-class>
    </validator>
    <validator>
        <validator-id>discountValidator</validator-id>
        <validator-class>jm.com.dpbennett.fm.validator.DiscountValidator</validator-class>
    </validator>
    <validator>
        <validator-id>supplierValidator</validator-id>
        <validator-class>jm.com.dpbennett.fm.validator.SupplierValidator</validator-class>
    </validator>
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>classificationDialog</from-outcome>
            <to-view-id>/finance/classificationDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobCategoryDialog</from-outcome>
            <to-view-id>/finance/jobCategoryDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobSubcategoryDialog</from-outcome>
            <to-view-id>/finance/jobSubcategoryDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>     
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sectorDialog</from-outcome>
            <to-view-id>/finance/sectorDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>serviceDialog</from-outcome>
            <to-view-id>/finance/serviceDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>accountingCodeDialog</from-outcome>
            <to-view-id>/finance/accountingCodeDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>taxDialog</from-outcome>
            <to-view-id>/finance/taxDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>discountDialog</from-outcome>
            <to-view-id>/finance/discountDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>currencyDialog</from-outcome>
            <to-view-id>/finance/currencyDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <managed-bean>
        <managed-bean-name>financeManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.fm.manager.FinanceManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
