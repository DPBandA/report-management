	<queryString>
		<![CDATA[SELECT
     jobsubcategory.`SubCategory`  AS jobsubcategory_SubCategory,
     CASE 
     WHEN(jobstatusandtracking.`DATEOFCOMPLETION` >=$P{startOfPeriod1}AND jobstatusandtracking.`DATEOFCOMPLETION` <=$P{endOfPeriod1}) THEN $P{period1Label}
     WHEN(jobstatusandtracking.`DATEOFCOMPLETION` >=$P{startOfPeriod2}AND jobstatusandtracking.`DATEOFCOMPLETION` <=$P{endOfPeriod2}) THEN $P{period2Label}
     WHEN(jobstatusandtracking.`DATEOFCOMPLETION` >=$P{startOfPeriod3}AND jobstatusandtracking.`DATEOFCOMPLETION` <=$P{endOfPeriod3}) THEN $P{period3Label}
     WHEN(jobstatusandtracking.`DATEOFCOMPLETION` >=$P{startOfPeriod4}AND jobstatusandtracking.`DATEOFCOMPLETION` <=$P{endOfPeriod4}) THEN $P{period4Label}
     WHEN(jobstatusandtracking.`DATEOFCOMPLETION` >=$P{startOfPeriod4}AND jobstatusandtracking.`DATEOFCOMPLETION` <=$P{endOfPeriod5}) THEN $P{period5Label}
     END AS Date_Period,
     SUM(jobcostingandpayment.`FINALCOST`) AS Earnings,
     department.`SUBGROUPCODE` AS department_SUBGROUPCODE,
     department.`NAME` AS department_NAME
FROM
     `department` department INNER JOIN `job` job ON department.`ID` = job.`DEPARTMENT_ID`
     INNER JOIN `jobsubcategory` jobsubcategory ON job.`JOBSUBCATEGORY_ID` = jobsubcategory.`ID`
     INNER JOIN `jobstatusandtracking` jobstatusandtracking ON job.`JOBSTATUSANDTRACKING_ID` = jobstatusandtracking.`ID`
     INNER JOIN `jobcostingandpayment` jobcostingandpayment ON job.`JOBCOSTINGANDPAYMENT_ID` = jobcostingandpayment.`ID`
     INNER JOIN `department` department_A ON job.`SUBCONTRACTEDDEPARTMENT_ID` = department_A.`ID`
WHERE
 ((jobstatusandtracking.`DATEOFCOMPLETION` >= $P{startOfPeriod1}
 AND jobstatusandtracking.`DATEOFCOMPLETION` <= $P{endOfPeriod1})
  OR (jobstatusandtracking.`DATEOFCOMPLETION` >= $P{startOfPeriod2}
 AND jobstatusandtracking.`DATEOFCOMPLETION` <= $P{endOfPeriod2})
  OR (jobstatusandtracking.`DATEOFCOMPLETION` >= $P{startOfPeriod3}
 AND jobstatusandtracking.`DATEOFCOMPLETION` <= $P{endOfPeriod3})
 OR (jobstatusandtracking.`DATEOFCOMPLETION` >= $P{startOfPeriod4}
 AND jobstatusandtracking.`DATEOFCOMPLETION` <= $P{endOfPeriod4})
  OR (jobstatusandtracking.`DATEOFCOMPLETION` >= $P{startOfPeriod5}
 AND jobstatusandtracking.`DATEOFCOMPLETION` <= $P{endOfPeriod5}))
 AND jobsubcategory.`IsEarning` = 1
 AND jobsubcategory.`active` = 1
 AND (department_A.`SUBGROUPCODE` = 0 OR department_A.`SUBGROUPCODE` IS NULL)
GROUP BY
     Date_Period,
     jobsubcategory_SubCategory
ORDER BY
     department_NAME ASC,
     jobsubcategory_SubCategory ASC,
     Date_Period ASC]]>
	</queryString>
